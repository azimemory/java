--CHAR 문자 입력하기
INSERT INTO TB_TYPE(CHAR_TEST) VALUES('HELLO');
SELECT CHAR_TEST FROM TB_TYPE;

--VARCHAR2 문자 입력하기
INSERT INTO TB_TYPE(VARCHAR2_TEST) VALUES('HELLO');
SELECT VARCHAR2_TEST FROM TB_TYPE;

--CLOB, BLOB에 데이터 입력하기
INSERT INTO TB_TYPE(CLOB_TEST) VALUES('CLOB HELLO!!');
SELECT * FROM TB_TYPE;

DELETE FROM TB_TYPE WHERE CLOB_TEST IS NOT NULL;

INSERT INTO TB_TYPE(BLOB_TEST) VALUES('123456789');
SELECT BLOB_TEST FROM TB_TYPE;








